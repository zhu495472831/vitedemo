{
  "name": "vitedemo",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && esno ./build/scripts/runBuildConfig.ts",
    "staging": "vue-tsc --noEmit && vite build --mode staging && esno ./build/scripts/runBuildConfig.ts",
    "serve": "npm run build && vite preview",
    "serve:dist": "vite preview",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.lintstagedrc",
    "reinstall": "rimraf package-lock.json && rimraf \"node_modules/!(rimraf|.bin)\" && npm run bootstrap",
    "bootstrap": "npm install",
    "report": "cross-env report=true npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "start-server-and-test dev http-get://localhost:3000 cypress",
    "cypress": "cypress run",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.7",
    "mockjs": "^1.1.0",
    "vue": "^3.2.6",
    "vue-i18n": "^9.2.0-beta.6",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-legacy": "^1.5.3",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.14",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "cypress": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.0",
    "jest": "^27.2.0",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "standard-version": "^9.3.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.2",
    "unplugin-vue-components": "^0.15.1",
    "vite": "^2.5.4",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.0",
    "vite-plugin-imagemin": "^0.4.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-windicss": "^1.4.3",
    "vue-tsc": "^0.2.2",
    "vue3-jest": "^27.0.0-alpha.1",
    "windicss": "^3.1.7"
  }
}
